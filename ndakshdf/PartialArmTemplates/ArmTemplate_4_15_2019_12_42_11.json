{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ndakshdf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/DestinationDataset_as4')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "this one is for testing.",
				"linkedServiceName": {
					"referenceName": "AzureSqlDatabase1",
					"type": "LinkedServiceReference"
				},
				"type": "AzureSqlTable",
				"structure": [
					{
						"name": "Index",
						"type": "Int32"
					},
					{
						"name": "Girth (in)",
						"type": "Decimal"
					},
					{
						"name": "Height (ft)",
						"type": "Decimal"
					},
					{
						"name": "Volume(ft^3)",
						"type": "Decimal"
					}
				],
				"typeProperties": {
					"tableName": "[concat('[', 'dbo].[Trees]')]"
				}
			}
		}
	]
}